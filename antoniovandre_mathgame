  while read -r LINHA; do FLAG="0"; FLAG2="0"; echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"; if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Mon" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Tue" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Wed" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Thu" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Fri" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sat" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sun" ]; then DATA=$LINHA; fi; TEMP=$(echo "${LINHA}" | grep "Eval: errou." | wc -c); TEMP2=$(echo "Eval: errou." | grep "Eval: errou." | wc -c); if [ "${TEMP}" == "${TEMP2}" ]; then FLAG="1"; fi; TEMP=$(echo "${LINHA}" | grep "Eval: acertou." | wc -c); TEMP2=$(echo "Eval: acertou." | grep "Eval: acertou." | wc -c); if [ "${TEMP}" == "${TEMP2}" ] && [ "${FLAG}" == "1" ]; then FLAG="2"; fi; if [ "${DATAP}" == "" ]; then DATAP=$DATA; fi; if [ $(echo "${DATA}" | sed 's/  / /g' | cut -d " " -f 2) ==  $(echo "${DATAP}" | sed 's/  / /g' | cut -d " " -f 2) ] && [ $(echo "${DATA}" | sed 's/  / /g' | cut -d " " -f 6) ==  $(echo "${DATAP}" | sed 's/  / /g' | cut -d " " -f 6) ]; then if [ "${FLAG}" == "1" ]; then CONTADOR=$(echo "${CONTADOR} + 1" | bc); fi; else FLAG2="1"; fi; if [ "${CONTADOR}" -gt "0" ] && [ "${FLAG2}" == "1" ]; then FLAG2="0"; echo -n "$(echo ${DATAP} | sed 's/  / /g' | cut -d ' ' -f 2)$(echo ${DATAP} | sed 's/  / /g' | cut -d ' ' -f 6)" | tr -d " " | tr -d "\n" >> "${ARQUIVOGNUPLOTDATA10}"; echo " ${CONTADOR}" >> "${ARQUIVOGNUPLOTDATA10}"; if [ "${MAXMESERROS}" -lt "${CONTADOR}" ]; then MAXMESERROS=$CONTADOR; fi; CONTADOR="0"; DATAP=$DATA; fi; done < "${ARQUIVOLOGTEMP}"; if [ "${FLAG}" == "1" ]; then echo -n "$(echo ${DATA} | sed 's/  / /g' | cut -d ' ' -f 2)$(echo ${DATA} | sed 's/  / /g' | cut -d ' ' -f 6)" | tr -d " " | tr -d "\n" >> "${ARQUIVOGNUPLOTDATA10}"; echo " 1" >> "${ARQUIVOGNUPLOTDATA10}"; CONTADOR="1"; if [ "${MAXMESERROS}" -lt "${CONTADOR}" ]; then MAXMESERROS=$CONTADOR; fi; fi

