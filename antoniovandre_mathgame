  if [ "${MAXDIA}" -gt "0" ]; then gnuplot -p -e "set xtics font 'Verdana,10'; set yrange [0:]; set boxwidth 0.3; set style fill solid; set xtics 1; set ytics 0,${MAXDIA}; set terminal png size 800,600; set xtics rotate by 90 right; set title 'Frequência diária de uso do game.'; show title; set output '${ARQUIVOGRAFICOTEMPO}'; plot '${ARQUIVOGNUPLOTDATA2}' using 2:xticlabels(1) notitle with lines lc -1" &>> /dev/null; else cp "${IMAGEGRAPH0}" "${ARQUIVOGRAFICOTEMPO}" &>> /dev/null; fi

